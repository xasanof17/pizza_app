{
  "expo": {
    "name": "pizza",
    "slug": "bandit pizza",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./static/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./static/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./static/splash.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./static/splash.png"
    }
  }
}
