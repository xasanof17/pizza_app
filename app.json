{
  "expo": {
    "name": "pizza",
    "slug": "pizza",
    "version": "1.0.0",
    "owner": "xasanof17",
    "description": "This is Pizza app.",
    "orientation": "portrait",
    "icon": "./static/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./static/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xasanof17.pizza"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./static/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.xasanof17.pizza"
    },
    "web": {
      "favicon": "./static/icon.png"
    },
    "plugins": [
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "26110229-0da0-4f97-978a-5f89e9e71420"
      }
    }
  }
}
